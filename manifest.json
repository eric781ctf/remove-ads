{
  "manifest_version": 3,
  "name": "Ad Remover & Pinner",
  "version": "1.1",
  "description": "Remove ads and pin them. Toggle on/off via popup.",
  "permissions": ["scripting", "activeTab", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.png"
  }
}
